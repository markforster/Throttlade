{
  "name": "throttlade",
  "version": "0.0.1",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^5.3.8",
    "esbuild": "^0.25.10",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build:theme": "sass --load-path=node_modules src/theme/bootstrap.scss:src/theme/bootstrap.css",
    "watch:theme": "sass --watch --load-path=node_modules src/theme/bootstrap.scss:src/theme/bootstrap.css",
    "dev:old": "npm run build:theme && node esbuild.config.mjs --watch",
    "dev": "node esbuild.config.mjs --watch",
    "dev:watch": " npm run dev && npm run watch:theme",
    "build:all": "  npm run build:theme && node esbuild.config.mjs",
    "build": "node esbuild.config.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve:demo": "http-server demo -p 5500 -c-1"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.12",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "esbuild": "^0.23.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.6.0",
    "http-server": "^14.1.1",
    "sass": "^1.79.4"
  },
