{
  "manifest_version": 3,
  "name": "Throttlade",
  "version": "0.0.2",
  "action": {
    "default_title": "Throttlade",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/favicon.ico",
      "32": "icons/favicon-96x96.png",
      "48": "icons/apple-touch-icon.png",
      "128": "icons/web-app-manifest-192x192.png"
    }
  },
  "icons": {
    "16": "icons/favicon.ico",
    "32": "icons/favicon-96x96.png",
    "48": "icons/apple-touch-icon.png",
    "128": "icons/web-app-manifest-192x192.png"
  },
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "inpage/inpage.js"
      ],
      "world": "MAIN",
      "run_at": "document_start"
    }
  ]
}
